{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "logAnalyticsEnabled": {
      "value": true
    },
    "logAnalyticsSku": {
      "value": "PerGB2018"
    },
    "logAnalyticsRetentionInDays": {
      "value": 30
    },
    "keyVaultEnabled": {
      "value": true
    },
    "storageAccountEnabled": {
      "value": true
    },
    "cosmosDbEnabled": {
      "value": true
    },
    "apiManagementEnabled": {
      "value": true
    },
    "appServicePlanEnabled": {
      "value": true
    },
    "appServiceEnabled": {
      "value": true
    },
    "openAiEnabled": {
      "value": true
    },
    "openAiDeployments": {
      "value": []
    },
    "openAiPublicNetworkAccess": {
      "value": "Enabled"
    },
    "cognitiveSearchEnabled": {
      "value": true
    },
    "containerRegistryEnabled": {
      "value": true
    },
    "iotHubEnabled": {
      "value": true
    },
    "virtualMachineEnabled": {
      "value": true
    },
    "virtualNetworkEnabled": {
      "value": true
    },
    "authenticationType": {
      "value": "sshPublicKey"
    },
    "vmAdminUsername": {
      "value": "azadmin"
    },
    "vmAdminPasswordOrKey": {
      "value": "<ssh-public-key>"
    },
    "tags": {
      "value": {
        "scenario": "example"
      }
    }
  }
}
